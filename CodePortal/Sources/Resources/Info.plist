<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleName</key>
	<string>Code Portal</string>
	<key>CFBundleIdentifier</key>
	<string>com.codeportal.app</string>
	<key>CFBundleVersion</key>
	<string>1</string>
	<key>CFBundleShortVersionString</key>
	<string>0.1.0</string>
	<key>LSMinimumSystemVersion</key>
	<string>14.0</string>
	<!-- URL scheme: codeportal:// — no-op handler in v1 (logs and drops all invocations) -->
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleURLName</key>
			<string>com.codeportal.url</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>codeportal</string>
			</array>
		</dict>
	</array>
	<!-- App Sandbox DISABLED — required for PTY. Cannot ship on Mac App Store. -->
	<key>com.apple.security.app-sandbox</key>
	<false/>
</dict>
</plist>
